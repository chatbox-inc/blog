    "npm-run-all": "^4.1.3",
    "nuxt": "^1.0.0",
    "processmd": "^4.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
