{
  "name": "blog",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "mikakane <t.goto@chatbox-inc.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "api": "run-s api:*",
    "api:clean": "rm -rf static/_api && mkdir static/_api",
    "api:generate_report": "processmd documents/**/*.md --stdout --outputDir static/_api > static/_api/summary.json",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "npm-run-all": "^4.1.3",
    "nuxt": "^1.0.0",
    "processmd": "^4.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0"
  }
}
